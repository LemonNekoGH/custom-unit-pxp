{
  "name": "@lemonneko/postcss-plugin-pxp",
  "type": "module",
  "version": "0.0.16",
  "author": "LemonnekoGH",
  "license": "MIT",
  "homepage": "https://github.com/LemonnekoGH/custom-unit-pxp",
  "repository": {
    "type": "git",
    "url": "https://github.com/LemonnekoGH/custom-unit-pxp.git"
  },
  "bugs": {
    "url": "https://github.com/LemonnekoGH/custom-unit-pxp/issues"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "postcss-plugin-pxp"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build.ts && bun --bun tsc",
    "prepublishOnly": "bun run build",
    "publish:run": "bun publish",
    "publish:dry-run": "bun publish --dry-run",
    "lint": "bun --bun eslint .",
    "lint:fix": "bun --bun eslint . --fix",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "peerDependencies": {
    "postcss": "^8.5.6",
    "postcss-value-parser": "catalog:",
    "typescript": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@lemonneko/pxp-transformer": "workspace:*",
    "bun-types": "catalog:",
    "eslint": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
